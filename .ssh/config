Host github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_ed25519_github
  IdentitiesOnly yes

  # Configuration for Control Node
Host control node
  HostName 18.204.230.160      # Replace with the control node's IP or hostname
  User ubuntu                  # Replace with the username on your control node
  IdentityFile ~/.ssh/id_rsa    # The SSH key used for the control node
  IdentitiesOnly yes

# Configuration for Managed Node 1
Host managed_node1
  HostName 54.226.176.246      # Replace with Managed Node 1's IP address or hostname
  User ubuntu                  # Replace with the username on Managed Node 1
  IdentityFile ~/.ssh/id_rsa    # The SSH key used for Managed Node 1 (same or different key)
  IdentitiesOnly yes

